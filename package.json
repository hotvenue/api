{
  "name": "hotvenue-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pm2:production": "pm2 startOrReload config/pm2/production.json",
    "prestart": "npm install",
    "start": "node ./bin/www",
    "start:production": "npm run prestart && npm run pm2:production",
    "deploy": "git pull",
    "deploy:production": "npm run deploy && npm run start:production",
    "dev": "nodemon ./bin/www",
    "eslint": "eslint .",
    "check": "npm run eslint",
    "mocha": "mocha --reporter spec --timeout 30s --bail test/index.js",
    "pretest": "npm install",
    "test": "npm run check && NODE_ENV=test npm run mocha",
    "test:cov": "nyc npm test"
  },
  "dependencies": {
    "aws-sdk": "^2.3.17",
    "body-parser": "~1.13.2",
    "config": "^1.20.1",
    "cookie-parser": "~1.3.5",
    "elasticsearch": "^11.0.1",
    "epilogue": "^0.6.6",
    "express": "~4.13.1",
    "fluent-ffmpeg": "^2.1.0",
    "http-aws-es": "^1.1.3",
    "jimp": "^0.2.24",
    "kue": "^0.11.1",
    "lodash": "^4.13.1",
    "markdown-it": "^7.0.0",
    "moment": "^2.13.0",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "mysql": "^2.10.2",
    "node-telegram-bot-api": "^0.23.3",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.4.2",
    "nodemailer-ses-transport": "^1.3.1",
    "pluralize": "^3.0.0",
    "request": "^2.72.0",
    "sequelize": "^3.23.0",
    "umzug": "^1.11.0",
    "winston": "^2.2.0",
    "winston-elasticsearch": "^0.4.0",
    "winston-sns": "^0.2.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4",
    "sqlite3": "^3.1.3",
    "supertest": "^1.2.0"
  }
}
