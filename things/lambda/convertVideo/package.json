{
  "name": "convertVideo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf node_modules",
    "build": "npm run pretest",
    "postbuild": "chmod +x ./ffprobe && chmod +x ./ffmpeg && zip -r convertVideo.zip .",
    "pretest": "npm run pretest:cp && npm run pretest:install",
    "pretest:cp": "cp -r ../../../config .",
    "pretest:install": "npm install",
    "test": "node test"
  },
  "dependencies": {
    "aws-sdk": "^2.5.4",
    "config": "^1.20.1",
    "sharp": "^0.16.0"
  }
}
