{
  "family": "hotvenue-server",
  "containerDefinitions": [
    {
      "image": "390360040979.dkr.ecr.eu-west-1.amazonaws.com/hotvenue-server:v_%BUILD_NUMBER%",
      "name": "hotvenue",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
